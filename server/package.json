{
  "name": "server",
  "version": "0.0.1",
  "description": "",
  "author": "",
  "private": true,
  "license": "UNLICENSED",
  "scripts": {
    "build": "nest build",
    "format": "prettier --write \"src/**/*.ts\" \"tests/**/*.ts\"",
    "start": "nest start",
    "start:dev": "nest start --watch",
    "start:debug": "nest start --debug --watch",
    "start:prod": "node dist/main",
    "lint": "eslint \"{src,tests}/**/*.ts\" --fix",
    "test": "NODE_ENV=test mocha --exit -r ts-node/register --r source-map-support/register --recursive ./tests/**/*.test.ts",
    "test:ci": "NODE_ENV=test nyc --reporter=html --reporter=json-summary  mocha --reporter mocha-allure-reporter --exit --timeout 5000 -r ts-node/register --recursive ./tests/**/*.test.ts"
  },
  "dependencies": {
    "@nestjs/axios": "^2.0.0",
    "@nestjs/common": "^11.0.16",
    "@nestjs/config": "^2.3.4",
    "@nestjs/core": "^9.4.3",
    "@nestjs/platform-express": "^10.4.18",
    "@nestjs/platform-fastify": "^10.4.5",
    "@nestjs/schedule": "^2.2.3",
    "@nestjs/sequelize": "^9.0.2",
    "axios": "^1.8.3",
    "class-transformer": "^0.5.1",
    "class-validator": "^0.14.1",
    "js-yaml": "^4.1.0",
    "pg": "^8.13.2",
    "reflect-metadata": "^0.2.2",
    "rxjs": "^7.8.2",
    "sequelize": "^6.37.4",
    "sequelize-typescript": "^2.1.6"
  },
  "devDependencies": {
    "@faker-js/faker": "^7.6.0",
    "@istanbuljs/nyc-config-typescript": "^1.0.2",
    "@nestjs/cli": "^9.0.0",
    "@nestjs/schematics": "^9.0.0",
    "@nestjs/testing": "^9.3.2",
    "@types/chai": "^4.3.4",
    "@types/cron": "^2.0.0",
    "@types/express": "^4.17.13",
    "@types/js-yaml": "^4.0.5",
    "@types/mocha": "^10.0.1",
    "@types/node": "18.11.18",
    "@types/node-cron": "^3.0.7",
    "@types/sequelize": "^4.28.14",
    "@types/sinon": "^10.0.13",
    "@typescript-eslint/eslint-plugin": "^5.0.0",
    "@typescript-eslint/parser": "^5.0.0",
    "chai": "^4.3.7",
    "chai-http": "^4.3.0",
    "eslint": "^8.0.1",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-prettier": "^4.0.0",
    "mocha": "^10.2.0",
    "mocha-allure-reporter": "^1.4.0",
    "nyc": "^15.1.0",
    "prettier": "^2.3.2",
    "sinon": "^15.0.1",
    "source-map-support": "^0.5.20",
    "ts-loader": "^9.2.3",
    "ts-node": "^10.0.0",
    "tsconfig-paths": "4.1.1",
    "typescript": "^4.7.4"
  },
  "nyc": {
    "extension": [
      ".ts",
      ".tsx"
    ],
    "reporter": [
      "html"
    ],
    "all": true,
    "include": [
      "src/**/*.ts"
    ],
    "exclude": [
      "tests/**/*.test.ts"
    ]
  }
}
