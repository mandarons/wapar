{
  "name": "app",
  "version": "0.0.1",
  "private": true,
  "scripts": {
    "dev": "bun run --bun vite dev",
    "build": "bun run --bun vite build",
    "preview": "bun run --bun vite preview",
    "test": "bun test",
    "check": "bunx svelte-kit sync && bunx svelte-check --tsconfig ./tsconfig.json",
    "check:watch": "bunx svelte-kit sync && bunx svelte-check --tsconfig ./tsconfig.json --watch",
    "lint": "bunx prettier --check . && bunx eslint .",
    "format": "bunx prettier --write .",
    "test:e2e": "bunx playwright test",
    "test:unit": "bun test"
  },
  "devDependencies": {
    "@playwright/test": "^1.28.1",
    "@skeletonlabs/skeleton": "2.7.1",
    "@skeletonlabs/tw-plugin": "0.3.1",
    "@sveltejs/adapter-auto": "^3.0.0",
    "@sveltejs/adapter-cloudflare": "^4.1.0",
    "@sveltejs/kit": "^2.0.0",
    "@sveltejs/vite-plugin-svelte": "^3.0.0",
    "@tailwindcss/typography": "0.5.10",
    "@types/eslint": "8.56.0",
    "@types/node": "20.11.6",
    "@typescript-eslint/eslint-plugin": "^6.0.0",
    "@typescript-eslint/parser": "^6.0.0",
    "autoprefixer": "10.4.17",
    "eslint": "^8.56.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-svelte": "^2.35.1",
    "prettier": "^3.0.0",
    "prettier-plugin-svelte": "^3.0.3",
    "svelte": "^4.2.7",
    "svelte-check": "^3.6.0",
    "tailwindcss": "^3.4.1",
    "typescript": "^5.0.0",
    "vite": "^5.0.0",
    "vitest": "^1.0.0"
  },
  "type": "module",
  "packageManager": "bun@1.0.30",
  "dependencies": {
    "svgmap": "^2.10.1"
  }
}
