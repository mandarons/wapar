---
import Layout from "../layouts/Layout.astro";
const response = await fetch(`https://wapar-api.mandarons.com/api/usage`);
const data = (await response.json());
---

<Layout title="Wapar :: Mandarons Apps Usage" currentPage="home" data={data}>
	<h1 class="text-center">{`Total:${data.totalInstallations}`}</h1>
</Layout>
