---
import Layout from "../layouts/Layout.astro";
const response = await fetch(`https://wapar-api.mandarons.com/api/data`);
const data = (await response.json()).data;
const appName = "icloud-drive-docker";
---

<Layout title="Wapar :: iCloud Drive Docker" currentPage={appName} data={data}>
    <h1 class="text-center">{`Total: ${data.iCloudDocker.total}`}</h1>
</Layout>
